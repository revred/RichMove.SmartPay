{
  "stryker-config": {
    "project": "RichMove.SmartPay.Core.csproj",
    "test-projects": ["./TESTS/Core.Tests/RichMove.SmartPay.Core.Tests.csproj"],
    "reporters": ["html", "progress", "dashboard"],
    "mutation-level": "Complete",
    "threshold-high": 95,
    "threshold-low": 90,
    "threshold-break": 85,
    "coverage-analysis": "perTest",
    "mutate": [
      "SOURCE/Core/**/*.cs",
      "!SOURCE/Core/**/Program.cs",
      "!SOURCE/Core/**/Migrations/*.cs"
    ],
    "dashboard-project": "github.com/revred/RichMove.SmartPay",
    "dashboard-version": "master",
    "dashboard-module": "Core",
    "concurrency": 4,
    "disable-bail": false
  }
}