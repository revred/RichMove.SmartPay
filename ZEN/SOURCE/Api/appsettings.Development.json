{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "WP4": {
    "Notifications": {
      "Enabled": true,
      "Provider": "InMemory"
    },
    "MultiTenancy": {
      "Enabled": false,
      "Strategy": "Host",
      "Header": "X-Tenant"
    },
    "Analytics": {
      "Enabled": true,
      "RequestLogging": false
    },
    "Triggers": {
      "FxQuoteCreated": false
    }
  },
  "WP5": {
    "Webhooks": {
      "Enabled": false,
      "Endpoints": [],
      "TimeoutSeconds": 5,
      "MaxAttempts": 5,
      "InitialBackoffMs": 300
    }
  }
}
